def printIncreasingPower(x):
    i=1
    while(i**2<=x):
        print(i**2, end=" ")
        i=i+1
def main():

    testcases = int(input())

    while(testcases > 0):
        x = int(input())
        
        printIncreasingPower(x);
        print ()        
        testcases -= 1
 
if __name__ == '__main__':
    main()
